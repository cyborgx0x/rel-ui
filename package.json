{
  "name": "eDashboard",
  "version": "0.0.1",
  "description": "React TypeScript Material-UI",
  "private": true,
  "homepage": ".",
  "main": "build/electron/main.js",
  "keywords": [
    "react",
    "react-wordcloud",
    "visualization",
    "d3js"
  ],
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@iconify/icons-ant-design": "^1.2.5",
    "@iconify/icons-eva": "^1.2.6",
    "@iconify/icons-ic": "^1.2.13",
    "@iconify/react": "^4.1.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.118",
    "@mui/material": "^5.0.0",
    "@mui/styles": "^5.11.7",
    "@mui/utils": "^5.11.7",
    "@mui/x-data-grid": "^5.17.21",
    "@react-oauth/google": "^0.11.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.15",
    "@types/latinize": "^0.2.15",
    "@types/lodash": "^4.14.191",
    "@types/material-ui": "^0.21.8",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.0.0",
    "@types/numeral": "^2.0.2",
    "@types/qs": "^6.9.7",
    "@types/react": "18",
    "@types/react-date-range": "^1.4.4",
    "@types/react-dom": "18",
    "@types/react-helmet": "^6.1.0",
    "@types/react-loading-overlay": "^1.0.1",
    "@types/yup": "^0.32.0",
    "add": "^2.0.6",
    "apexcharts": "^3.36.3",
    "axios": "^0.21.0",
    "crypto-es": "^1.2.7",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "formik": "^2.2.9",
    "hamburger-react": "^2.4.1",
    "http-proxy-middleware": "^2.0.6",
    "jwt-decode": "^3.1.2",
    "latinize": "^0.5.0",
    "moment": "^2.29.4",
    "notistack": "^2.0.8",
    "numeral": "^2.0.6",
    "qs": "^6.11.0",
    "react": "18",
    "react-apexcharts": "^1.4.0",
    "react-date-range": "^1.4.0",
    "react-dom": "18",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.5",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux-persist": "^6.0.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4",
    "yarn": "^1.22.19",
    "yup": "^1.0.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:dev": "env-cmd -f .env.development react-app-rewired --max_old_space_size=4096 build",
    "build:test": "env-cmd -f .env.test react-app-rewired --max_old_space_size=4096 build",
    "build:prod": "env-cmd -f .env.production react-app-rewired --max_old_space_size=4096 build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "electron .",
    "electron:package:mac": "env-cmd -f .env.production react-app-rewired --max_old_space_size=4096 build && tsc -p electron && electron-builder -m -c.extraMetadata.main=build/electron/main.js",
    "electron:package:win": "env-cmd -f .env.production react-app-rewired --max_old_space_size=4096 build && tsc -p electron && electron-builder -c.extraMetadata.main=build/electron/main.js",
    "electron:package:build": "env-cmd -f .env.production react-app-rewired --max_old_space_size=4096 build && tsc -p electron && electron-builder --ia32 --x64 -w -c.extraMetadata.main=build/electron/main.js",
    "electron:package:linux": "yarn build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "postinstall": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "customize-cra": "^0.9.1",
    "electron": "^23.1.4",
    "electron-builder": "^23.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^10.0.8",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "react-app-rewired": "^2.1.5",
    "sass": "^1.50.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  }
}
